  "devDependencies": {
    "commitlint": "^17.4.4",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "gts": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
