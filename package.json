{
  "name": "tightscript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install && yarn run compile",
    "lint": "eslint .",
    "clean": "gts clean",
    "compile": "rm -Rf ./build && tsc && cp -R src/proto/ build/src/proto",
    "fix": "eslint . --fix",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint",
    "test": "jest --coverage NODE_PATH=./build/src",
    "build": "yarn run compile",
    "start": "node build/src/index.js",
    "watch": "nodemon src/index.ts --watch src"
  },
